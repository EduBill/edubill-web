{
  "name": "edubill_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite --host",
    "build": "vite build",
    "build:prod": "cross-env VUE_APP_MODE=production yarn build",
    "build:staging": "cross-env VUE_APP_MODE=staging yarn build",
    "format": "prettier --check \"src/**/*.vue\"",
    "lint": "eslint \"**/*.{ts,tsx,vue}\" --fix",
    "start": "yarn serve",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{html,css,scss,js,ts,json,vue}": "prettier --write",
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.8",
    "core-js": "^3.8.3",
    "eslint-plugin-import": "^2.29.1",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-require": "^1.1.14",
    "vite-plugin-require-transform": "^1.0.21"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "packageManager": "yarn@1.22.21"
}
